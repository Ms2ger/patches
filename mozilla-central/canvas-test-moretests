+ok(ctx.font === '20px serif', "ctx.font === '20px serif'");
+
+ctx.font = '20px serif';
+ctx.font = '10px initial';
+todo(ctx.font === '20px serif', "ctx.font === '20px serif'");
+
+ctx.font = '20px serif';
+ctx.font = '10px default';
+todo(ctx.font === '20px serif', "ctx.font === '20px serif'");
+
+ctx.font = '20px serif';
+ctx.font = '10px inherit';
+ok(ctx.font === '20px serif', "ctx.font === '20px serif'");
+
+ctx.font = '20px serif';
+ctx.font = '1em serif; background: green; margin: 10px';
+ok(ctx.font === '20px serif', "ctx.font === '20px serif'");
+
+
+}
+</script>
